{
  "name": "gsheet.action",
  "version": "0.0.0",
  "description": "A github action to help with CRUD operations on google sheets.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "document": "tsc bin/* --outDir lib && node ./lib/bin/documentation.js",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint */**/*.ts --quiet --fix",
    "clean": "rm -rf dist && rm -rf lib",
    "package": "ncc build --external bin --source-map --license licenses.txt",
    "test": "jest --coverage",
    "all": "npm run clean && pm run build && npm run format && npm run lint && npm run package && npm test",
    "test:watch": "jest --watch --coverage",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jroehl/gsheet.action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "jroehl <mail@johannroehl.de>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run document"
    }
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "google-sheet-cli": "^1.4.1",
    "google-spreadsheet": "^3.0.13",
    "husky": "^4.3.0",
    "semantic-release": "^17.2.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.8",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@vercel/ncc": "^0.23.0",
    "eslint": "^7.14.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^3.14.0",
    "prettier": "2.1.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  }
}
