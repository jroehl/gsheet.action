{
  "name": "gsheet.action",
  "version": "0.0.0",
  "description": "A github action to help with CRUD operations on google sheets.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "document": "tsc src/bin/* --outDir lib && node ./lib/bin/documentation.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jroehl/gsheet.action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "jroehl <mail@johannroehl.de>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run document"
    }
  },
  "dependencies": {
    "@actions/core": "^1.2.4",
    "google-sheet-cli": "^1.4.1",
    "google-spreadsheet": "^3.0.11",
    "husky": "^4.2.5",
    "semantic-release": "^17.1.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "jest": "^26.2.2",
    "jest-circus": "^26.2.2",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  }
}
